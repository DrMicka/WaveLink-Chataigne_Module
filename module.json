{
	"name": "WaveLink",
	"type": "WebSocket Client",
	"path": "Software",
	"version": "0.5.2",
	"description": "To control your sounds via WaveLink",
	"url": "https://github.com/DrMicka/WaveLink-Chataigne_Module",
	"downloadURL": "https://github.com/DrMicka/WaveLink-Chataigne_Module/archive/master.zip",
	"hasInput": true,
	"hasOutput": true,
	"hideDefaultCommands": true,
	"hideDefaultParameters": [
		"autoAdd",
		"connected",
		"protocol",
		"messageStructure",
		"firstValueIsTheName",
		"useSecureConnection",
		"serverPath",
	],
   "parameters": {
   },
   "defaults": {
      "Protocol": "Lines",
      "autoAdd": false,
      "serverPath": "127.0.0.1:1824",
   },
	"values": {
		"removeAllValues": {
			"type":"Trigger",
		},
		"AudioChannelNumber": {
			"type":"Float",
			"readOnly":true,
		}
	},
   "scripts": [
      "WaveLink.js"
   ],
   "commands": {
		"Set volume": {
			"menu": "",
			"setupCallback":"setupEnumChannel",
			"callback": "setVolume",
			"parameters": {
				"Volume (%)": {
					"type": "Integer", "min":0, "max":100,
				},
			}
		},

		"Toggle mute volume": {
			"menu": "",
			"setupCallback":"setupEnumChannel",
			"callback": "toggleMuteVolume",
			"parameters": {
				
			}
		},
		
		"Toggle filter": {
			"menu": "",
			"callback": "toggleFilter",
			"setupCallback":"setupToggleFilter",
			"parameters": {
			
			},
		},
		"Script command": {
			"menu": "",
			"callback": "Commande",
			"parameters": {
				"data": {
            },}
		},
	}
}

