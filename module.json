{
   "name": "WaveLink",
   "type": "WebSocket Client",
   "path": "Software",
   "version": "0.5.0",
   "description": "To control your WaveLink sounds",
   "url": "https://github.com/DrMicka/WaveLink-Chataigne_Module",
   "downloadURL": "https://github.com/DrMicka/WaveLink-Chataigne_Module/archive/master.zip",
   "hasInput": true,
   "hasOutput": true,
   "hideDefaultCommands": true,
   "parameters": {
   },
   "defaults": {
      "Protocol": "Lines",
      "autoAdd": false,
      "serverPath": "127.0.0.1:1824",
   },
   "scripts": [
      "WaveLink.js"
   ],
   "commands": {
		"Commande": {
			"menu": "",
			"callback": "Commande",
			"parameters": {
				"data": {
            },}
		},
		
		"Set volume": {
			"menu": "",
			"setupCallback":"setupEnumChannel",
			"callback": "setVolume",
			"parameters": {

				"Volume (%)": {
					"type": "Integer", "min":0, "max":100,
				},
			}
		},

		"Toggle mute volume": {
			"menu": "",
			"setupCallback":"setupEnumChannel",
			"callback": "toggleMuteVolume",
			"parameters": {
				
			}
		},
		
		"Toggle filter": {
			"menu": "",
			"callback": "toggleFilter",
			"setupCallback":"setupToggleFilter",
			"parameters": {
			
			},
		},
	}
}

