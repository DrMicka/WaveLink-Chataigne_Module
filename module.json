{
   "name": "WaveLink",
   "type": "WebSocket Client",
   "path": "Software",
   "version": "0.0.1",
   "description": "To control your WaveLink sounds",
   "url": "https://github.com/DrMicka/WaveLink-Chataigne_Module",
   "downloadURL": "https://github.com/DrMicka/WaveLink-Chataigne_Module/archive/master.zip",
   "hasInput": true,
   "hasOutput": true,
   "hideDefaultCommands": true,
   "parameters": {
   },
   "defaults": {
      "Protocol": "Lines",
      "autoAdd": false,
      "serverPath": "127.0.0.1:1824",
   },
   "scripts": [
      "WaveLink.js"
   ],
   "commands": {
		"Hello Wave Link": {
			"menu": "",
			"callback": "HelloWaveLink",
			"parameters": {
            },
		},
		
		"Raw Command (JSON)": {
			"menu": "",
			"callback": "sendObsRawJSONCommand",
			"parameters": {
				"data": {
					"type": "String"
            },}
		},
		
		"Commande": {
			"menu": "",
			"callback": "Commande",
			"parameters": {
				"data": {
            },}
		},
		
		"Set volume": {
			"menu": "",
			"callback": "setVolume",
			"parameters": {
				"Toggle channel": {
					"type": "Enum", "options": { 
						"Wave micro":"waveMicro",
						"System":"System",
						"Music":"Music",
						"Browser":"Browser",
						"Voice Chat":"voiceChat",
						"SFX":"SFX",
						"Game":"Game",
						"Aux1":"AUX1",
						"Aux2":"AUX2",
						}
				},
				"Type of volume": {
					"type": "Enum", "options": { 
						"Local mix":"local",
						"Stream mix":"stream",}
				},
				"Volume (%)": {
					"type": "Integer", "min":0, "max":100,
				},
			}
		},

		"Toggle mute volume": {
			"menu": "",
			"callback": "toggleMuteVolume",
			"parameters": {
				"Toggle channel": {
					"type": "Enum", "options": { 
						"Wave micro":"waveMicro",
						"System":"System",
						"Music":"Music",
						"Browser":"Browser",
						"Voice Chat":"voiceChat",
						"SFX":"SFX",
						"Game":"Game",
						"Aux1":"AUX1",
						"Aux2":"AUX2",
						}
				},
				"Type of volume": {
					"type": "Enum", "options": { 
						"Local mix":"local",
						"Stream mix":"stream",
					}
				},
			}
		},
		
		"Toggle filter": {
			"menu": "",
			"callback": "toggleFilter",
			"parameters": {
				"Toggle channel": {
					"type": "Enum", "options": { 
						"Wave micro":"waveMicro",
						"System":"System",
						"Music":"Music",
						"Browser":"Browser",
						"Voice Chat":"voiceChat",
						"SFX":"SFX",
						"Game":"Game",
						"Aux1":"AUX1",
						"Aux2":"AUX2",
						}
				},
				"Filter to toggle": {
					"type": "Enum", "options": { 
						"Local mix":"local",
						"Stream mix":"stream",
					}
				},
			}
		},
	}
}
